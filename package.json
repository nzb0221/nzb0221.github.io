{
  "name": "reveal.js",
  "version": "4.1.3",
  "description": "The HTML Presentation Framework",
  "homepage": "https://revealjs.com",
  "subdomain": "revealjs",
  "main": "dist/reveal.js",
  "module": "dist/reveal.esm.js",
  "license": "MIT",
  "scripts": {
    "test": "gulp test",
    "start": "gulp serve",
    "build": "gulp build"
  },
  "author": {
    "name": "Hakim El Hattab",
    "email": "hakim.elhattab@gmail.com",
    "web": "https://hakim.se"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/hakimel/reveal.js.git"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "keywords": [
    "reveal",
    "slides",
    "presentation"
  ],
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/preset-env": "^7.16.8",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "babel-plugin-transform-html-import-to-string": "2.0.0",
    "colors": "^1.4.0",
    "core-js": "^3.20.2",
    "fitty": "^2.3.6",
    "glob": "^7.2.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-connect": "^5.7.0",
    "gulp-eslint": "^6.0.0",
    "gulp-header": "^2.0.9",
    "gulp-tap": "^2.0.0",
    "gulp-zip": "^5.1.0",
    "highlight.js": "^11.4.0",
    "marked": "^4.0.10",
    "node-qunit-puppeteer": "^2.1.0",
    "qunit": "^2.17.2",
    "reveal-badges": "^1.2.2",
    "reveal.js-drawer": "^0.1.3",
    "reveal.js-fsfx": "^1.1.1",
    "reveal.js-pointer": "^0.1.3",
    "rollup": "^2.63.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.48.0",
    "yargs": "^17.3.1"
  },
  "browserslist": "> 0.5%, IE 11, not dead",
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "sourceType": "commonjs",
      "allowImportExportEverywhere": true,
      "requireConfigFile": false
    },
    "globals": {
      "module": false,
      "console": false,
      "unescape": false,
      "define": false,
      "exports": false
    },
    "rules": {
      "curly": 0,
      "eqeqeq": 2,
      "wrap-iife": [
        2,
        "any"
      ],
      "no-use-before-define": [
        2,
        {
          "functions": false
        }
      ],
      "new-cap": 2,
      "no-caller": 2,
      "dot-notation": 0,
      "no-eq-null": 2,
      "no-unused-expressions": 0
    }
  },
  "dependencies": {
    "@types/reveal": "^4.1.1",
    "reveal_external": "^1.3.0",
    "reveal-code-focus": "^1.1.0",
    "reveal-plantuml": "^1.0.2",
    "reveal-run-in-terminal": "^1.0.5",
    "reveal.js": "^4.2.1",
    "reveal.js-appearance": "^1.1.1",
    "reveal.js-copycode": "^1.0.3",
    "reveal.js-d3js": "^1.0.0",
    "reveal.js-menu": "^2.1.0",
    "reveal.js-plugins": "^4.1.5",
    "reveal.js-toolbar": "^0.2.1",
    "reveald3": "^1.5.5",
    "typed-js": "^0.2.3"
  }
}
